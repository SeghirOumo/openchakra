  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "eslint src/**/*.{ts,tsx} --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(svg|jpg|png|css)$": "<rootDir>/empty-module.js",
      "~(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(js)$": "babel-jest",
      "^.+\\.ts(x)?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s(x)?",
      "!**/node_modules/**"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    }
  }
