{
  "name": "expo-app",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "run:android": "TAMAGUI_TARGET=native yarn expo run:android",
    "run:ios": "TAMAGUI_TARGET=native yarn expo run:ios",
    "build:android": "TAMAGUI_TARGET=native eas build -p android",
    "build:android:dev:local": "TAMAGUI_TARGET=native eas build -p android --local --profile development",
    "build:android:prod:local": "TAMAGUI_TARGET=native eas build -p android --local --profile production",
    "build:ios": "TAMAGUI_TARGET=native eas build -p ios",
    "web": "TAMAGUI_TARGET=web yarn expo start --web",
    "eject": "expo eject",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@react-native-community/datetimepicker": "6.5.2",
    "@types/react-native": "~0.70.6",
    "app": "*",
    "babel-plugin-module-resolver": "^4.1.0",
    "eas-cli-local-build-plugin": "^0.0.128",
    "expo": "~47.0.12",
    "expo-checkbox": "~2.2.2",
    "expo-constants": "^14.0.2",
    "expo-dev-client": "^2.0.1",
    "expo-document-picker": "~11.0.1",
    "expo-font": "^11.0.1",
    "expo-linear-gradient": "^12.0.1",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "^1.4.2",
    "expo-updates": "^0.15.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "~0.18.7",
    "react-native-webview": "11.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@expo/metro-config": "^0.3.21",
    "@tamagui/babel-plugin": "1.2.12",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "typescript": "^4.7.4"
  }
}
